{
  "name": "boilerplate-react",
  "version": "1.0.0",
  "author": "VandrÃ© Leal",
  "license": "MIT",
  "homepage": "https://vandreleal.github.io/boilerplate-react",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vandreleal/boilerplate-react.git"
  },
  "bugs": {
    "url": "https://github.com/vandreleal/boilerplate-react/issues"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format": "yarn lint:fix && yarn prettier",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@apollo/client": "3.6.9",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "28.1.8",
    "@types/node": "18.7.13",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "5.1.26",
    "graphql": "16.6.0",
    "graphql.macro": "1.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "styled-components": "5.3.5",
    "typescript": "4.8.2",
    "web-vitals": "3.0.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "6.5.10",
    "@storybook/addon-actions": "6.5.10",
    "@storybook/addon-essentials": "6.5.10",
    "@storybook/addon-interactions": "6.5.10",
    "@storybook/addon-links": "6.5.10",
    "@storybook/builder-webpack5": "6.5.10",
    "@storybook/manager-webpack5": "6.5.10",
    "@storybook/node-logger": "6.5.10",
    "@storybook/preset-create-react-app": "4.1.2",
    "@storybook/react": "6.5.10",
    "@storybook/testing-library": "0.0.13",
    "@trivago/prettier-plugin-sort-imports": "3.3.0",
    "babel-plugin-named-exports-order": "0.0.2",
    "eslint-config-vandre": "1.1.0",
    "husky": "8.0.1",
    "jest-styled-components": "7.1.0",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "webpack": "5.74.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "vandre"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
